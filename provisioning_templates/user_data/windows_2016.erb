identity:
  Sysprep:
    guiRunOnce:
      commandList:
        - '<%= powershell %> -Command "invoke-webrequest -Uri https://repo.hpc.osc.edu/internal/install_puppet.ps1 -Outfile C:\install_puppet.ps1"'
        - "<%= powershell.gsub(/\\/,'\&\&') %> -Command \"& '.\\install_puppet.ps1 <%= @host.name %>'\""
        - '<%= powershell %> -Command "invoke-webrequest -Uri <%= foreman_url("built") %>"'
    guiUnattended:
      autoLogon: true
      autoLogonCount: 1
      password:
        plainText: false
        value: ''
      timeZone: 35
<%- if host_param_true?('provision-join-domain') -%>
    identification:
      domainAdmin: 'Administrator'
      domainAdminPassword:
        plainText: false
        value: ''
      joinDomain: 'ms.osc.edu'
<%- end -%>
    licenseFilePrintData:
      autoMode: 'perSeat'
    userData:
      computerName: <%= @host.shortname %>
      fullName: 'OSC'
      orgName: 'Ohio Supercomputer Center'
      productId: '<%= host_param('windows_key') %>'
nicSettingMap:
  - adapter:
      dnsDomain: <%= @host.domain %>
      dnsServerList:
        - 192.157.5.137
        - 192.157.5.138
      gateway:
        - <%= @host.subnet.gateway %>
      ip: <%= @host.ip %>
      subnetMask: <%= @host.subnet.mask %>
globalIPSettings:
  dnsServerList:
    - 192.157.5.137
    - 192.157.5.138
  dnsSuffixList:
    - ten.osc.edu
    - osc.edu
    - hpc.osc.edu
    - epn.osc.edu
options:
  changeSID: true
  deleteAccounts: false
